<h4>New Team</h4>

<div class="row">
  <div class="col s6 m8 offset-m2">
    <%= simple_form_for @team, url: [:admin, @team] do |f| %>
      <%= f.input :name, wrapper_html: { class: 's12' } %>
      <%= f.input :logo, as: :file, wrapper_html: { class: 's12' } %>
      <%= f.button :submit %>
    <% end %>
  </div>
</div>
