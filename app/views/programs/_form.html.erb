<%= simple_form_for @program, html: {class: "form-horizontal"} do |f| %>
  <%= f.error_notification %>

  <%= f.input :name %>
  <%= f.input :abbr %>
  <%= f.input :icon, as: :image_preview, input_html: { preview_version: :medium } %>
  <%= f.input :logo, as: :image_preview, input_html: { preview_version: :medium } %>
  <%= f.button :submit %>

<% end %>
